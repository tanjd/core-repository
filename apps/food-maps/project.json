{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "food-maps",
  "projectType": "application",
  "sourceRoot": "apps/food-maps",
  "tags": [],
  "targets": {
    "build": {
      "configurations": {
        "development": {
          "outputPath": "apps/food-maps"
        },
        "production": {}
      },
      "defaultConfiguration": "production",
      "executor": "@nx/next:build",
      "options": {
        "outputPath": "dist/apps/food-maps"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "configurations": {
        "development": {
          "buildTarget": "food-maps:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "food-maps:build:production",
          "dev": false
        }
      },
      "defaultConfiguration": "development",
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "food-maps:build",
        "dev": true,
        "port": 3000
      }
    }
  }
}
